    <para styleclass="Code" style="line-height:1.00;"><text styleclass="Code" translate="true">hibernate.connection.driver_class org.postgresql.Driver</text></para>
    <para styleclass="Code" style="line-height:1.00;"><text styleclass="Code" translate="true">hibernate.connection.username postgres</text></para>
    <para styleclass="Code" style="line-height:1.00;"><text styleclass="Code" translate="true">hibernate.connection.password postgres</text></para>
