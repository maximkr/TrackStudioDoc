    <para styleclass="Code Example_workflow" style="text-align:left; text-indent:0px; margin-top:0px; margin-right:0px; margin-bottom:0px; margin-left:0px; line-height:1.0; background-color:#ffffcc; white-space:nowrap; page-break-inside:auto; page-break-after:auto; border:none; tabstops:none;"><text styleclass="workflow" style="font-family:&apos;Courier New&apos;; font-size:10pt; font-weight:normal; font-style:normal; text-decoration:none; text-transform:none; vertical-align:baseline; color:#666666; background-color:transparent; letter-spacing:normal; letter-scaling:100%;" translate="true"> &#160; &#160; &#160; &#160; &#160; &#160;long l = now.getTimeInMillis() - update.getTimeInMillis();</text></para>
    <para styleclass="Code Example_workflow" style="text-align:left; text-indent:0px; margin-top:0px; margin-right:0px; margin-bottom:0px; margin-left:0px; line-height:1.0; background-color:#ffffcc; white-space:nowrap; page-break-inside:auto; page-break-after:auto; border:none; tabstops:none;"><text styleclass="workflow" style="font-family:&apos;Courier New&apos;; font-size:10pt; font-weight:normal; font-style:normal; text-decoration:none; text-transform:none; vertical-align:baseline; color:#666666; background-color:transparent; letter-spacing:normal; letter-scaling:100%;" translate="true"> &#160; &#160; &#160; &#160; &#160; &#160;return (int) (l / (24 * 60 * 60 * 1000));</text></para>
    <para styleclass="Code Example_workflow" style="text-align:left; text-indent:0px; margin-top:0px; margin-right:0px; margin-bottom:0px; margin-left:0px; line-height:1.0; background-color:#ffffcc; white-space:nowrap; page-break-inside:auto; page-break-after:auto; border:none; tabstops:none;"><text styleclass="workflow" style="font-family:&apos;Courier New&apos;; font-size:10pt; font-weight:normal; font-style:normal; text-decoration:none; text-transform:none; vertical-align:baseline; color:#666666; background-color:transparent; letter-spacing:normal; letter-scaling:100%;" translate="true"> &#160; &#160; &#160; &#160;} else return 0;</text></para>
    <para styleclass="Code Example_workflow" style="text-align:left; text-indent:0px; margin-top:0px; margin-right:0px; margin-bottom:0px; margin-left:0px; line-height:1.0; background-color:#ffffcc; white-space:nowrap; page-break-inside:auto; page-break-after:auto; border:none; tabstops:none;"><text styleclass="workflow" style="font-family:&apos;Courier New&apos;; font-size:10pt; font-weight:normal; font-style:normal; text-decoration:none; text-transform:none; vertical-align:baseline; color:#666666; background-color:transparent; letter-spacing:normal; letter-scaling:100%;" translate="true"> &#160; &#160;}</text></para>
